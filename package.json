{
  "name": "langchain-ts-starter",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:server": "nodemon --watch 'src/**/*.ts' --exec 'node --loader ts-node/esm --require tsconfig-paths/register' server/index.ts",
    "dev": "npm run dev:server && npm run dev:app",
    "tsup": "tsup",
    "build": "tsup",
    "index": "tsx -r dotenv/config src/index.ts",
    "recommend": "tsx -r dotenv/config src/recommend.ts",
    "lint": "eslint src",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --list-different \"**/*.ts\"",
    "dev:app": "vite --config app/vite.config.ts",
    "build:app": "tsc app && vite build app",
    "lint:app": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "keywords": [
    "starter",
    "template",
    "node",
    "typescript",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@pinecone-database/pinecone": "^0.1.6",
    "@xenova/transformers": "^2.2.0",
    "chalk": "^5.2.0",
    "cli-progress": "^3.12.0",
    "console-table-printer": "^2.11.1",
    "cross-fetch": "^3.1.6",
    "dotenv": "^16.1.3",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.0-beta.1",
    "nodemon": "^3.0.1",
    "onnxruntime-node": "^1.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tablemark": "^3.0.0",
    "tsup": "^6.7.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/cli-progress": "^3.11.0",
    "@types/express": "^4.17.17",
    "@types/js-yaml": "^4",
    "@types/json2csv": "^5.0.3",
    "@types/node": "^18.17.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.3.3",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.12.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.7",
    "vite-plugin-windicss": "^1.9.0",
    "windicss": "^3.5.6"
  }
}
